{
    "name": "zaimea/socialite-provider",
    "license": "MIT",
    "homepage": "https://zaimea.com",
    "description": "The official Laravel Zaimea Accounts Driver for Socialite.",
    "keywords": [
        "laravel",
        "accounts",
        "socialite",
        "php",
        "sdk",
        "zaimealabs",
        "zaimea"
    ],
    "authors": [
        {
            "name": "Custură Laurențiu",
            "email": "mail@custura.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "guzzlehttp/guzzle": "^7.7",
        "laravel/socialite": "^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpstan/phpstan": "^1.12.23",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Zaimea\\Socialite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zaimea\\Socialite\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zaimea\\Socialite\\ZaimeaServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
